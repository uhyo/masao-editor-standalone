{
    "compilerOptions":{
        "lib": ["dom", "es2015"],
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "jsx":"react",
        "module":"commonjs",
        "target":"es5",
        "typeRoots": ["./node_modules/@types"]
    },
    "exclude": ["node_modules"]
}



